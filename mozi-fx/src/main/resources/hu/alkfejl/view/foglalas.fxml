<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.alkfejl.view.controller.FoglalasokController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="649.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="&lt; Főmenü">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#fomenu" text="Vissza" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <TableView fx:id="tabla" prefHeight="375.0" prefWidth="625.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="nevCol" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Név" />
                <TableColumn fx:id="filmCol" maxWidth="140.0" minWidth="140.0" prefWidth="140.0" text="Film" />
                  <TableColumn fx:id="idoCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Időpont" />
                  <TableColumn fx:id="teremCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Terem" />
                  <TableColumn fx:id="sorCol" maxWidth="45.0" minWidth="45.0" prefWidth="45.0" text="Sor" />
                  <TableColumn fx:id="oszlopCol" maxWidth="45.0" minWidth="45.0" prefWidth="45.0" text="Oszlop" />
                  <TableColumn fx:id="torolCol" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="Töröl" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>

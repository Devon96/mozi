<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.alkfejl.view.controller.TeremController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="&lt; Főmenü">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#fomenu" text="Vissza" />
                     </items></Menu>
                <Menu mnemonicParsing="false" text="Szerkesztés">
                  <items>
                        <MenuItem mnemonicParsing="false" text="Unspecified Action" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <TableView fx:id="tabla" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <columns>
                  <TableColumn fx:id="nevCol" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" text="Termnév" />
                  <TableColumn fx:id="sorCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Sorokszáma" />
                  <TableColumn fx:id="oszlopCol" maxWidth="110.0" minWidth="110.0" prefWidth="110.0" text="Oszlopok száma" />
                  <TableColumn fx:id="helyCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Férőhely" />
               </columns></TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
